<%= render "layouts/errors", :object => @project %>

<%= form_for [@skill, @project] do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :github_link %>
  <%= f.text_field :github %>
  <p><%= f.submit %></p>
<% end %>
